
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <tr>
            <th>Product Id</th>
            <th>Product Title</th>
            <th>Brand</th>
            <th>Description</th>
            <th>Date</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        <tr ng-repeat="item in product">
            <td>{{ item.Id }}</td>
            <td>{{ item.Title }}</td>
            <td>{{ item.Brand }}</td>
            <td>{{ item.ShortDescription }}</td>
            <td>{{ DatePublished }}</td>
            <td>
                <a href="#/edit/{{item.Id}}" class="glyphicon glyphicon-dashboard"></a>
            </td>
            <td>
                <a href="#/edit/{{item.Id}}" class="glyphicon glyphicon-edit"></a>
            </td>
            <td>
                <a href="#/delete/{{item.Id}}" class="glyphicon glyphicon-trash"></a>
            </td>
        </tr>
    </table>
</div>  