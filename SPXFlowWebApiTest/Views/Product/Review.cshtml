
<div class="table-responsive">
    <table class="table table-striped table-bordered">
        <tr>
            <th>Review Id</th>
            <th>Rating </th>
            <th>Comment</th>
            <th>User</th>
            <th>Product ID</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        <tr ng-repeat="item in product">
            <td>{{ item.ReviewID }}</td>
            <td>{{ item.Rating }}</td>
            <td>{{ item.Comment }}</td>
            <td>{{ item.User }}</td>
            <td>{{ item.ProductID }}</td>
            <td>
                <a href="#/edit/{{item.Id}}" class="glyphicon glyphicon-dashboard"></a>
            </td>
            <td>
                <a href="#/edit/{{item.Id}}" class="glyphicon glyphicon-edit"></a>
            </td>
            <td>
                <a href="#/delete/{{item.Id}}" class="glyphicon glyphicon-trash"></a>
            </td>
        </tr>
    </table>
</div>  